<?xml version='1.0' encoding='UTF-8'?>
<?xml-stylesheet type="text/xsl" href="Doc.xsl"?> 
<class name="MultiRegExpLexer" language="JavaScript">
	<desc>.</desc>
	<syntax name="Syntaxe 1">
		<desc>Récupération direct du résultat d'un scanne.</desc>
		<code><![CDATA[ var eScanResult = MultiRegExpLexer( sText, sSyntax ) ]]></code>
		<arguments>
			<arg name="sText" type="Number">
				<desc>Texte à scanner.</desc>
			</arg>
			<arg name="sSyntax" type="Number">
				<desc>Syntaxe à utiliser.</desc>
			</arg>
		</arguments>
	</syntax>
	<syntax name="Syntaxe 2">
		<desc>Décomposition des étapes du scanne.</desc>
		<code><![CDATA[
var oLexer = new MultiRegExpLexer()

// utilisation 1
oLexer.init( sText, sSyntax )
while( oLexer.readToken()) ;

// utilisation 2
eScanResult = oLexer.scan( sText, sSyntax )
]]></code>
	</syntax>

	<group name="Instance(s) MultiRegExpLexer">
		<properties name="Attributs">
			<variable name="bSkip" type="Boolean">
				<desc>.</desc>
			</variable>
		</properties>
		<methods name="Méthodes">
			<function name="appendNode">
				<desc>. </desc>
				<arguments>
					<arg type="HTMLElement" name="eNode">
						<desc>.</desc>
					</arg>
				</arguments>
				<returnValue type="this"></returnValue>
			</function>
			<function name="end">
				<desc>.</desc>
				<returnValue type="HTMLElement"></returnValue>
			</function>
			<function name="init">
				<desc>. </desc>
				<arguments>
					<arg type="String" name="sText">
						<desc>.</desc>
					</arg>
					<arg type="String" name="sSyntax">
						<desc>.</desc>
					</arg>
				</arguments>
				<returnValue type="this"></returnValue>
			</function>
			<function name="readToken">
				<desc>.</desc>
				<returnValue type="Mixte"></returnValue>
			</function>
			<function name="scan">
				<desc>.</desc>
				<arguments>
					<arg type="String" name="sText">
						<desc>.</desc>
					</arg>
					<arg type="String" name="sSyntax">
						<desc>.</desc>
					</arg>
				</arguments>
				<returnValue type="Mixte"></returnValue>
			</function>
			<function name="searchToken">
				<desc>.</desc>
				<arguments>
					<arg type="?" name="oRE">
						<desc>.</desc>
					</arg>
				</arguments>
				<returnValue type="String"></returnValue>
			</function>
			<function name="setSyntax">
				<desc>.</desc>
				<arguments>
					<arg type="String" name="sSyntax">
						<desc>.</desc>
					</arg>
				</arguments>
			</function>
		</methods>
	</group>
	<group name="MultiRegExpLexer.Rules">
	</group>
</class>